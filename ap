import java.util.*;
public class Solution {
    public static List< Integer > sortedArray(int []a, int []b) {
        int m=a.length;
        int n=b.length;
      HashSet<Integer> set = new HashSet<>();  
        for(int i=0;i<m;i++){
            set.add(a[i]);
        }
        for(int i=0;i<n;i++){
            set.add(b[i]);
        }
       //  Iterator<Integer> iterator = set.iterator();  
       
      return new ArrayList<>(set) ;
    }
}
